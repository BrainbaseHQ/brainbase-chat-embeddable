{
  "name": "@brainbase-labs/chat-widget",
  "version": "0.1.5",
  "description": "A React chat widget for embedding Brainbase Labs AI agents in your applications",
  "author": "Brainbase Labs <hello@brainbaselabs.com>",
  "license": "MIT",
  "homepage": "https://github.com/BrainbaseHQ/brainbase-chat-embeddable#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrainbaseHQ/brainbase-chat-embeddable.git"
  },
  "bugs": {
    "url": "https://github.com/BrainbaseHQ/brainbase-chat-embeddable/issues"
  },
  "keywords": [
    "react",
    "chat",
    "widget",
    "ai",
    "brainbase",
    "chatbot",
    "embeddable",
    "component",
    "web-component",
    "vanilla-js"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/embed.js",
  "jsdelivr": "./dist/embed.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./embed": {
      "import": "./dist/embed.js",
      "require": "./dist/embed.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": [
    "*.css",
    "./dist/embed.js"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && npm run build:embed",
    "build:lib": "tsc && vite build",
    "build:embed": "vite build --config vite.embed.config.ts",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.16.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.4.7",
    "terser": "^5.37.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0",
    "vite": "^6.0.3",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.3.0"
  }
}
